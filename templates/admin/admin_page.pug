html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css" integrity="sha512-HK5fgLBL+xu6dm/Ii3z4xhlSUyZgTT9tuc/hSrtw6uzJOvgRr2a9jyxxT1ely+B+xFAmJKVSTbpM/CuL7qxO8w==" crossorigin="anonymous")
        link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous")
        //- indiquer le chemin du dossier css a affilier au pug
        link(rel="stylesheet", href="/css/style.css")
        title #{websiteName}
        block css
        //- title Document
    body 
        .container
                header
                    nav.navbar.navbar-light.bg-light.p-3
                        div.d-flex.col-12.col-md-3.col-lg-2.mb-2.mb-lg-0.flex-wrap.flex-md-nowrap.justify-content-between
                            a.navbar-brand(href="#") Admin Panel                            
                        button.navbar-toggler.d-md-none.collapsed.mb-3(type="button" data-toggle="collapse" data-target="#sidebar" aria-controls="sidebar" aria-expanded="false" aria-label="Toggle navigation")  Boutton
                            span.navbar-toggler-icon                       
                        div.col-12.col-md-4.col-lg-2
                            input.form-control.form-control-dark(type="text" placeholder="Search" aria-label="Search")
                        div.col-12.col-md-5.col-lg-8.d-flex.align-items-center.justify-content-md-end.mt-3.mt-md-0
                            div.mr-3.mt-1
                                a.github-button(href="https://github.com/themesberg/simple-bootstrap-5-dashboard")(data-color-scheme="no-preference: dark; light: light; dark: light;" data-icon="octicon-star" data-size="large" data-show-count="true" aria-label="Star/themesberg/simple-bootstrap-5-dashboard") Star
                            div.dropdown
                                button.btn.btn-secondary.dropdown-toggle(type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-expanded="false") Hello, John Doe
                                ul.dropdown-menu.aria-labelledby="dropdownMenuButton"
                                    li
                                        a.dropdown-item(href="#") Settings
                                    li
                                        a.dropdown-item(href="#") Messages
                                    li
                                        a.dropdown-item(href="#") Sign out
